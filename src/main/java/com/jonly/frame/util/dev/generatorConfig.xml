<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<context id="mysql" defaultModelType="flat">
		<property name="autoDelimitKeywords" value="true" />
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<!-- <plugin type="com.ea.message.common.mybatis.plugin.PaginationPlugin"></plugin> -->
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
		<plugin type="com.jonly.frame.util.dev.MyBatisGenPlugin"></plugin>
		<!-- <plugin
			type="com.ea.message.common.mybatis.plugin.DeleteByExampleMysqlBugFixedPlugin"></plugin> -->
			
		<!-- 不自动生成注释 -->
	  	<commentGenerator>
	  		<property name="suppressAllComments" value="true"/>
	  	</commentGenerator>
	  	
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/test?characterEncoding=UTF-8"
			userId="root" password="jonly" />

	  	<!-- 类型转换器,这里使用能转换成基本类型的转换器 -->
	    <!-- <javaTypeResolver type="org.mybatis.generator.internal.types.JavaTypeResolverImpl">
	    	<property name="forceBigDecimals" value="false"/>
	    </javaTypeResolver> -->
		
		<javaModelGenerator targetPackage="com.jonly.test.model"
			targetProject="src/main/java" />
		
		<!-- *Mapper.xml生成器.若不需要,则将targetproject设为空值即可 -->
		<sqlMapGenerator targetPackage="conf-mybatis"
			targetProject="src/main/resources" />
		
		<!-- *Mapper.java接口生成器.若不需要,则将targetproject设为空值即可 -->
		<javaClientGenerator targetPackage="com.jonly.test.dao.mapper"
			targetProject="src/main/java" type="XMLMAPPER" />

		<table tableName="test" domainObjectName="Test" alias="test" 
			enableCountByExample="false" enableDeleteByExample="false"
            enableSelectByExample="false" enableUpdateByExample="false">
            
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
			<columnRenamingRule searchString="^T" replaceString=""/>
		</table>


		<!-- <table tableName="group" domainObjectName="GroupBase" alias="mgroup" 
			delimitIdentifiers="true"> -->
		<!-- <generatedKey column="id" sqlStatement="MySql" identity="true" /> -->
		<!-- <columnOverride column="is_del" javaType="java.lang.Integer"></columnOverride> -->
		<!-- <columnOverride column="state" javaType="java.lang.Integer"></columnOverride> -->
		<!-- </table> -->

	</context>
</generatorConfiguration>
